<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
 
<configuration>
       
    <typeAliases>
        <typeAlias type="net.my.user.User" alias="User" />
        <typeAlias type="net.my.board.Notice" alias="Notice"/>
        <typeAlias type="net.my.board.Notice" alias="Contact"/>
        <typeAlias type="net.my.main.Board" alias="Board"/>
    </typeAliases>
 
    <mappers>
        <mapper resource="net/my/mybatis/UserMapper.xml" />
        <mapper resource="net/my/mybatis/BoardMapper.xml" />
        <mapper resource="net/my/mybatis/main.xml"/>
        <mapper resource="net/my/mybatis/board.xml"/>
    </mappers>
 
</configuration>