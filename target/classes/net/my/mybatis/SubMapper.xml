<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="net.my.mybatis.SubMapper">
	<insert id="insert">
        INSERT INTO upload (member_no, board_no, file)
        VALUES (#{member_no}, #{board_no}, #{file})
    </insert>
</mapper>